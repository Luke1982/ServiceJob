<tables>
	<table>
		<name>vtiger_servicejob</name>
		<sql><![CDATA[CREATE TABLE `vtiger_servicejob` (
				  `servicejobid` INT(11) NOT NULL,
				  `servicejob_no` VARCHAR(56) DEFAULT NULL,
				  `service_procedure` VARCHAR(255) DEFAULT NULL,
				  `servicejob_status` VARCHAR(255) DEFAULT NULL, 
				  `servicejob_productname` VARCHAR(255) DEFAULT NULL, 
				  `servicejob_for_serial` VARCHAR(255) DEFAULT NULL, 
				  `mechanic_remarks` MEDIUMTEXT DEFAULT NULL, 
				  `servicejob_report` BLOB DEFAULT NULL, 
				  `execution_date` DATE,
				  `related_asset_id` INT(11), 
				  `related_so_id` INT(11), 
				  PRIMARY KEY (`servicejobid`)
				) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
	</table>
	<table>
		<name>vtiger_servicejobcf</name>
		<sql><![CDATA[CREATE TABLE `vtiger_servicejobcf` (
		  `servicejobid` int(11) NOT NULL,
		  PRIMARY KEY (`servicejobid`)
		) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
	</table>
</tables>
